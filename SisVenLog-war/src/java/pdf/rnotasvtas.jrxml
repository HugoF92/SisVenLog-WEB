	<queryString>
		<![CDATA[

SELECT n.nro_nota,
       n.fdocum,
       n.fac_ctipo_docum,
       n.cconc,
       n.texentas,
       n.tgravadas,
       n.timpuestos,
       f.nrofact,
       f.ctipo_docum,
       c.cod_cliente,
       c.xnombre,
       n.ttotal,
       f.cod_vendedor,
       e.xnombre  AS xnombre_vendedor,
       d.xdesc    AS xdesc_conc,
       f.ttotal   AS fac_ttotal,
       f.ffactur,
       n.cod_entregador,
       f.cod_zona,
       e2.xnombre AS xnombre_entregador,
       n.mestado
FROM   notas_ventas n
       INNER JOIN facturas f
               ON n.fac_ctipo_docum = f.ctipo_docum
                  AND n.nrofact = f.nrofact
                  AND n.ffactur = f.ffactur
                  AND n.cod_empr = f.cod_empr
       LEFT OUTER JOIN empleados e2
                    ON f.cod_empr = e2.cod_empr
                       AND n.cod_entregador = e2.cod_empleado
       LEFT OUTER JOIN empleados e
                    ON f.cod_vendedor = e.cod_empleado
                       AND f.cod_empr = e.cod_empr
       INNER JOIN clientes c
               ON n.cod_cliente = c.cod_cliente
                  AND n.cod_cliente = c.cod_cliente
       INNER JOIN conceptos_documentos d
               ON n.cconc = d.cconc
                  AND n.ctipo_docum = d.ctipo_docum
                  AND n.cconc = d.cconc,
       (SELECT DISTINCT n.nro_nota
        FROM   notas_ventas n,
               notas_ventas_det d
        WHERE  n.cod_empr = 2
               AND n.fdocum BETWEEN '2020-07-14' AND '2020-07-14'
               AND n.ctipo_docum = 'NCV'
               AND d.ctipo_docum = 'NCV'
               AND n.ctipo_docum = 'ncv'
               AND n.nro_nota = d.nro_nota
               AND n.fdocum = d.fdocum
               AND n.cod_empr = d.cod_empr
               AND n.mestado = 'A') t
WHERE  n.cod_empr = 2
       AND f.cod_empr = 2
       AND n.ctipo_docum = 'NCV'
       AND ( n.fdocum BETWEEN '2020-07-14' AND '2020-07-14' )
       AND ( n.ctipo_docum = 'ncv' )
       AND n.nro_nota = t.nro_nota
       AND n.mestado = 'A'
ORDER  BY n.cconc,
          n.fdocum,
          n.nro_nota ]]>
	</queryString>
