	<queryString>
		<![CDATA[SELECT *, (select [dbo].[sp_Dinero_a_Texto](icheque)) as totalletra
FROM CHEQUES_EMITIDOS A, proveedores b
 WHERE nro_cheque BETWEEN $P{desde}  AND $P{hasta}
AND  cod_banco = $P{banco}
