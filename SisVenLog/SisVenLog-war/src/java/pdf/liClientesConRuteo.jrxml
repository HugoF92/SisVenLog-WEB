	<parameter name="camposCondicionales" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[(($P{codTipoCliente} != null) ? (" AND c.ctipo_cliente = '" + $P{codTipoCliente} + "' "): "")
	+ (($P{codZona} != null )? (" AND r.cod_zona = '" + $P{codZona} + "' "): "")
	+ (($P{codRuta} != null) ? (" AND c.cod_ruta = " + $P{codRuta} + " "): "")
	+ (($P{estado} != null && $P{estado} == 1) ? (" AND cod_estado = 'A' ") :  "")
	+ (($P{estado} != null && $P{estado} == 2) ? (" AND CONVERT(char(10), c.falta, 103) BETWEEN '" + $P{fechaAltaDesde} + "' AND '" + $P{fechaAltaHasta} + "' "): "")
	+ (($P{estado} != null && $P{estado} == 3) ? (" AND cod_estado = 'I' "): "")
	+ (($P{listaCodClientes} != null && $P{listaCodClientes}.length() > 0)? (" AND c.cod_cliente IN (" + $P{listaCodClientes} + ") " ) : "")
	+ " ORDER BY c.xnombre"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT  c.cod_cliente, 
        r.cod_zona, 
        c.cod_ruta, 
        c.xnombre, 
        c.xpropietario, 
        c.xruc, 
        c.xdirec, 
        c.xtelef, 
        z.xdesc as xdesc_zona, 
        r.xdesc as xdesc_ruta,
        c.ctipo_cliente, 
        t.xdesc as xdesc_tipo, 
        cod_estado
FROM clientes c, tipos_clientes t, zonas z, rutas r
WHERE
    c.ctipo_cliente = t.ctipo_cliente
    AND c.cod_empr = r.cod_empr
    AND z.cod_empr = 2
    AND r.cod_zona = z.cod_zona
    AND r.cod_empr = z.cod_empr
    AND c.cod_ruta = r.cod_ruta
    $P!{camposCondicionales}]]>
	</queryString>
